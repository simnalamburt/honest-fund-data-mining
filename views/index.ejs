<!doctype html>
<html>
<head>
    <title>Index</title>
</head>
<body>
    <ul>
        <% for(var i = 0; i < messages.length; ++i){ %>
            <li><%= messages[i] %></li>
        <% } %>
    </ul>
    <% if(user){ %>
        <h1>Logged as <%= user.email %></h1>
    <% }else{ %>
        <h1>Not Logged in</h1>
    <% } %>
</body>
</html>
