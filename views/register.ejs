<!doctype html>
<html>
<head>
    <title>Register</title>
</head>
<body>
    <ul>
    <% for(var i = 0; i < messages.length; ++i){ %>
        <li><%= messages[i] %></li>
    <% } %>
    </ul>
    <form action="/register" method="post">
        <label>Email</label>
        <input type="email" name="email">
        <label>Password</label>
        <input type="password" name="password">
        <button type="submit">Register</button>
    </form>
</body>
</html>
